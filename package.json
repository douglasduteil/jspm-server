{
  "name": "jspm-server",
  "version": "1.0.0-alpha",
  "description": "JSPM development server prototype",
  "keywords": [
    "front-end",
    "development",
    "tool",
    "server",
    "http",
    "http2",
    "jspm",
    "cli"
  ],
  "author": "Glen Maddern",
  "bin": {
    "jspm-server": "bin/cli.js"
  },
  "main": "lib/index.js",
  "files": [
    "bin/",
    "lib/"
  ],
  "scripts": {
    "watch": "npm run watch:start --silent & npm run watch:build --silent",
    "test": "mocha",
    "start": "node bin/cli.js",
    "watch:start": "nodemon --exec 'npm start --silent' -w lib -w bin -w package.json",
    "watch:build": "nodemon --exec 'npm run build --silent' -w .babelrc -w src -e 'js,json'",
    "build": "babel src -d lib",
    "lint": "eslint bin src"
  },
  "engines": {
    "node": ">=0.8.0"
  },
  "preferGlobal": true,
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "^6.0.16",
    "connect": "^3.4.0",
    "morgan": "^1.6.1",
    "portscanner": "^1.0.0",
    "spdy": "^2.0.5",
    "url": "^0.11.0",
    "yargs": "^3.29.0"
  },
  "devDependencies": {
    "babel-cli": "^6.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.0.14",
    "babel-preset-es2015": "^6.1.2",
    "chai": "^3.4.0",
    "eslint": "^1.9.0",
    "eslint-config-standard": "^4.4.0",
    "eslint-plugin-standard": "^1.3.1",
    "hide-stack-frames-from": "^1.0.0",
    "mocha": "^2.3.3",
    "nodemon": "^1.8.1",
    "send": "^0.13.0",
    "supertest": "^1.1.0"
  }
}
