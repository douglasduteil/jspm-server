{
  "name": "jspm-server",
  "version": "1.0.0-alpha",
  "description": "JSPM development server prototype",
  "keywords": [
    "front-end",
    "development",
    "tool",
    "server",
    "http",
    "http2",
    "jspm",
    "cli"
  ],
  "author": "Glen Maddern",
  "bin": {
    "jspm-server": "bin/jspm-server-cli.js"
  },
  "main": "lib/index.js",
  "files": [
    "bin/",
    "lib/"
  ],
  "scripts": {
    "build": "babel src -d lib",
    "lint": "eslint bin src",
    "jspm": "jspm",
    "test": "babel-node ./node_modules/.bin/tape 'test/**/*.spec.js' | tap-spec",
    "watch:build": "npm run build --silent -- -w",
    "watch:test": "chokidar '+(src|test)/**/*.js' -c 'npm test --silent' --initial"
  },
  "engines": {
    "node": ">=0.8.0"
  },
  "preferGlobal": true,
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "^6.0.16",
    "chokidar": "^1.2.0",
    "connect": "^3.4.0",
    "fancy-log": "^1.1.0",
    "glogg": "^1.0.0",
    "hijackresponse": "^1.0.2",
    "lodash.ary": "^3.0.2",
    "lodash.assign": "^3.2.0",
    "micromatch": "^2.3.2",
    "morgan": "^1.6.1",
    "portscanner": "^1.0.0",
    "serve-static": "^1.10.0",
    "spdy": "^2.0.5",
    "systemjs": "^0.19.5",
    "systemjs-builder": "^0.14.10",
    "through2": "^2.0.0",
    "url": "^0.11.0",
    "yargs": "^3.29.0"
  },
  "devDependencies": {
    "babel-cli": "^6.1.2",
    "babel-core": "^6.1.4",
    "babel-plugin-transform-object-rest-spread": "^6.0.14",
    "babel-preset-es2015": "^6.1.2",
    "chokidar-cli": "^1.1.1",
    "eslint": "^1.9.0",
    "eslint-config-standard": "^4.4.0",
    "eslint-plugin-standard": "^1.3.1",
    "hide-stack-frames-from": "^1.0.0",
    "jspm": "^0.16.14",
    "send": "^0.13.0",
    "sinon": "^1.17.2",
    "supertest": "^1.1.0",
    "tap-spec": "^4.1.0",
    "tape": "^4.2.2"
  },
  "jspm": {
    "dependencies": {},
    "devDependencies": {
      "babel": "npm:babel-core@^5.8.24",
      "babel-runtime": "npm:babel-runtime@^5.8.24",
      "core-js": "npm:core-js@^1.1.4"
    }
  }
}
